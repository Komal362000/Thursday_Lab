package T_L_03_11_2022;



class Main_Class // main class
{
	public static void main(String[] args) // main method
	{
		Problem_Class pc = new Problem_Class();                                   //  object of Problem class
		Producer_Class p1 = new Producer_Class(pc, 1);           //  object of Producer_class and passing the arguments to the constructor
		Consumer_Class c1 = new Consumer_Class(pc, 2);  //  object of Consumer_class and passing the arguments to the constructor
	      c1.start(); // calling a run method
	      p1.start(); // calling a run method
	}
}
